
@{
    ViewData["Title"] = "ShowBookingToAdmin2";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div style="padding-left:100px; width:80%">
    <div class="row">
        <div class="col-6">
            <h3 class=" text-info">ShowBookingToAdmin</h3>
        </div>

    </div>
    <br />
    

    <div>
        <table class="table table-striped table-bordered ">
            <tr class="table-primary">
                <th>Booking No</th>
                <th>Customer Name</th>
                <th>PhoneNo</th>
                <th>Email</th>
                <th>Address</th>
                <th>DuplexName</th>
                <th>Photo</th>

            </tr>
            @foreach (var item in ViewBag.Show)
            {
            <tr>
                <td>@item.BookingNo</td>
                <td>@item.Name</td>
                <td>@item.PhoneNO</td>
                <td>@item.Email</td>
                <td>@item.Address</td>
                <td>@item.DuplexName</td>
                <td>
                    <img src="@item.Photo" style="height:100px;width:150px" />
                </td>
            </tr>
            }
        </table>
    </div>
</div>
